<h1>Vehicles</h1>
<table id='myTable'>
  <thead>
    <tr>
      <th>Vin</th>
      <th>Registeration number</th>
      <th>Status</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicles.each do |vehicle| %>
      <tr>
        <td><%= vehicle.vin %></td>
        <td><%= vehicle.registeration_number %></td>
        <td><%= vehicle.status %></td>
        <td><%= vehicle.customer.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>